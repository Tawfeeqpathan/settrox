
    <table class="table">
      <thead>
        <tr>
          <th>Order ID</th>
          <th>Customer</th>
          <th>Total Price</th>
          <th>Status</th>
        </tr>
      </thead>
      <tbody>
        <% orders.forEach(order => { %>
          <tr>
            <td><%= order.id %></td>
            <td><%= order.customer %></td>
            <td>$<%= order.totalPrice %></td>
            <td><%= order.status %></td>
          </tr>
        <% }) %>
      </tbody>
    </table>

    